<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="s" class="com.main.model.Student" autowire="constructor">
    <property name="rollno" value="1"></property>
    <property name="name" value="Siddarth"></property>
     
    </bean>
    <bean id="addr" class="com.main.model.Address">
    <property name="aeraName" value="VimanNagar"></property>
    <property name="cityName" value="Pune"></property>
    
        
    </bean>
    
        
    </beans>